{
  "expo": {
    "slug": "uha",
    "version": "1.13.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios-light.png",
    "scheme": "uha",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "appleTeamId": "WU9ZX7QZRL",
      "supportsTablet": true,
      "bundleIdentifier": "com.keireira.uha",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "uha",
          "уха"
        ],
        "UIDesignRequiresCompatibility": false,
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleAllowMixedLocalizations": true,
        "NSUserNotificationsUsageDescription": "This app uses notifications to notify you about upcoming payments"
      },
      "icon": {
        "dark": "./assets/images/ios-dark.png",
        "light": "./assets/images/ios-light.png"
      }
    },
    "locales": {
      "en": "./assets/locales/en.json",
      "ru": "./assets/locales/ru.json",
      "kk": "./assets/locales/kk.json",
      "es": "./assets/locales/es.json",
      "pt-BR": "./assets/locales/pt-BR.json",
      "de": "./assets/locales/de.json",
      "ja": "./assets/locales/ja.json",
      "fr": "./assets/locales/fr.json",
      "ko": "./assets/locales/ko.json",
      "it": "./assets/locales/it.json",
      "pl": "./assets/locales/pl.json",
      "uk": "./assets/locales/uk.json",
      "ka": "./assets/locales/ka.json",
      "bg": "./assets/locales/bg.json",
      "cs": "./assets/locales/cs.json",
      "da": "./assets/locales/da.json",
      "el": "./assets/locales/el.json",
      "fi": "./assets/locales/fi.json",
      "fil": "./assets/locales/fil.json",
      "hi": "./assets/locales/hi.json",
      "hu": "./assets/locales/hu.json",
      "is": "./assets/locales/is.json",
      "nb": "./assets/locales/nb.json",
      "nl": "./assets/locales/nl.json",
      "ro": "./assets/locales/ro.json",
      "sk": "./assets/locales/sk.json",
      "sr": "./assets/locales/sr.json",
      "sv": "./assets/locales/sv.json",
      "th": "./assets/locales/th.json",
      "vi": "./assets/locales/vi.json",
      "zh-Hans": "./assets/locales/zh-Hans.json",
      "zh-Hant": "./assets/locales/zh-Hant.json"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-dev-client",
      "expo-localization",
      [
        "@howincodes/expo-dynamic-app-icon",
        {
          "enby": {
            "ios": "./assets/images/enby-icon.png",
            "android": "./assets/images/enby-icon.png"
          },
          "lesbi": {
            "ios": "./assets/images/lesbi-icon.png",
            "android": "./assets/images/lesbi-icon.png"
          },
          "pan": {
            "ios": "./assets/images/pan-icon.png",
            "android": "./assets/images/pan-icon.png"
          },
          "trans": {
            "ios": "./assets/images/trans-icon.png",
            "android": "./assets/images/trans-icon.png"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "Notifications"
          ]
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "supportsRTL": false,
      "forcesRTL": false,
      "eas": {
        "projectId": "85064a70-6ca6-4a3a-91f3-6feb0c705943"
      }
    },
    "android": {
      "package": "com.keireira.uha"
    }
  }
}
